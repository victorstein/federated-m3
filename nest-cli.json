{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/federated-m3/src",
  "monorepo": true,
  "root": "apps/federated-m3",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/federated-m3/tsconfig.app.json"
  },
  "projects": {
    "federated-m3": {
      "type": "application",
      "root": "apps/federated-m3",
      "entryFile": "main",
      "sourceRoot": "apps/federated-m3/src",
      "compilerOptions": {
        "tsConfigPath": "apps/federated-m3/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "email": {
      "type": "application",
      "root": "apps/email",
      "entryFile": "main",
      "sourceRoot": "apps/email/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email/tsconfig.app.json"
      }
    },
    "role": {
      "type": "application",
      "root": "apps/role",
      "entryFile": "main",
      "sourceRoot": "apps/role/src",
      "compilerOptions": {
        "tsConfigPath": "apps/role/tsconfig.app.json"
      }
    }
  }
}